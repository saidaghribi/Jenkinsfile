pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
          stage('BUILD') {
            steps {
                echo 'Developpment'
            }
        }
          stage('TEST') {
            steps {
                echo 'Testing'
            }
        }
          stage('DEPLOY') {
            steps {
                echo 'La mise en production'
            }
        }
     
    }
}
